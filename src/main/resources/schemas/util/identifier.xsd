<?xml version="1.0"?>
<xs:schema 
    xmlns:xs="http://www.w3.org/2001/XMLSchema" 
    xmlns="https://github.com/Daomephsta/inscribe"  
    targetNamespace="https://github.com/Daomephsta/inscribe" elementFormDefault="qualified">
    
    <xs:simpleType name="identifier">
        <xs:restriction base="xs:string">
            <xs:pattern value="[a-z0-9_.-]+:[a-z0-9_.-/]+"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="model_identifier">
        <xs:restriction base="xs:string">
            <xs:pattern value="[a-z0-9_.-]+:[a-z0-9_.-/]+(#[a-z0-9_.-]+)?"/>
        </xs:restriction>
    </xs:simpleType>
</xs:schema>