<?xml version="1.0"?>
<xs:schema 
    xmlns:xs="http://www.w3.org/2001/XMLSchema" 
    xmlns="https://github.com/Daomephsta/inscribe"  
    targetNamespace="https://github.com/Daomephsta/inscribe" elementFormDefault="qualified">
    
    <xs:simpleType name="simple_number_array">
        <xs:restriction base="xs:string">
            <xs:pattern value="(\d+,\s*)*\d+"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="simple_string_array">
        <xs:restriction base="xs:string">
            <xs:pattern value="(\w+,\s*)*\w+"/>
        </xs:restriction>
    </xs:simpleType>
</xs:schema>